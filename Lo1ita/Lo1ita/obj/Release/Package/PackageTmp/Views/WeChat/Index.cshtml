    <!DOCTYPE html>
<html lang="zh-cn">
<head>
   
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width,initial-scale=1,user-scalable=0" />
    <title>WechatTest</title>
    <link rel="stylesheet" href="https://res.wx.qq.com/open/libs/weui/1.1.2/weui-for-work.css" />
</head>
<body ontouchstart>
    <div class="container">
        <div class="weui-cells weui-cells_checkbox">
            <label class="weui-cell weui-check__label" for="s11">
                <div class="weui-cell__hd">
                    <input type="checkbox" class="weui-check" name="checkbox1" id="s11" checked="checked">
                    <i class="weui-icon-checked"></i>
                </div>
                <div class="weui-cell__bd">
                    <p>小米</p>
                </div>
            </label>
            <label class="weui-cell weui-check__label" for="s12">
                <div class="weui-cell__hd">
                    <input type="checkbox" name="checkbox1" class="weui-check" id="s12">
                    <i class="weui-icon-checked"></i>
                </div>
                <div class="weui-cell__bd">
                    <p>华为</p>
                </div>
            </label>
            <a href="javascript:void(0);" class="weui-cell weui-cell_link">
                <div class="weui-cell__bd">添加更多</div>
            </a>
            <div class="weui-cell">
                <div class="weui-cell__hd"><label class="weui-label">UserTicket</label></div>
                <div class="weui-cell__bd">
                    <label class="weui-label" id="UserTicket">@ViewBag.UserTicket</label>
                </div>
            </div>
            <div class="weui-cell">
                <div class="weui-cell__hd"><label class="weui-label">AccessToken</label></div>
                <div class="weui-cell__bd">
                    <label class="weui-label" id="AccessToken">@ViewBag.AccessToken</label>
                </div>
            </div>
            
            <div class="weui-cell">
                <div class="weui-cell__hd"><label class="weui-label">id</label></div>
                <div class="weui-cell__bd">
                    <label class="weui-label" id="userid">@ViewBag.UserInfo.userid</label>
                </div>
            </div>
            <div class="weui-cell">
                <div class="weui-cell__hd"><label class="weui-label">姓名</label></div>
                <div class="weui-cell__bd">
                    <label class="weui-label" id="name">@ViewBag.UserInfo.name</label>
                </div>
            </div>
            <div class="weui-cell">
                <div class="weui-cell__hd"><label class="weui-label">手机</label></div>
                <div class="weui-cell__bd">
                    <label class="weui-label" id="mobile">@ViewBag.UserInfo.mobile</label>
                </div>
            </div>
            <div class="weui-cell">
                <div class="weui-cell__hd"><label class="weui-label">邮箱</label></div>
                <div class="weui-cell__bd">
                    <label class="weui-label" id="email">@ViewBag.UserInfo.email</label>
                </div>
            </div>
        </div>
    </div>
    <script src="~/Scripts/jquery-3.2.1.min.js"></script>
  
</body></html>




